# VISITS 
CREATE TABLE VISITS(EXHIBITIONID CHAR(50),VISITORID CHAR(50),DATEOFEXHIBUTION DATE ,PRIMARY KEY(EXHIBITIONID,VISITORID),FOREIGN KEY(EXHIBITIONID) REFERENCES EXHIBITION,FOREIGN KEY(VISITORID) REFERENCES VISITANT);
