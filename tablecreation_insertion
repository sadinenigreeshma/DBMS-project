#BILL TABLE:
CREATE TABLE BILL(BILLID CHAR(50),ARTID CHAR(50),DATE_OF_PURCHASE DATE,PRIMARY KEY(BILLID,ARTID),FOREIGN KEY(ARTID) REFERENCES ARTGALLERY,FOREIGN KEY(BILLID) REFERENCES ORDERS);
INSERT INTO BILL VALUES(12111,'ART1','12-OCT-2020');
