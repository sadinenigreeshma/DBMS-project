# ORGANISE TABLE:
CREATE TABLE ORGANSIES(ARTID CHAR(50),EXHIBITIONID CHAR(50),PRIMARY KEY(EXHIBITIONID,ARTID),FOREIGN KEY(ARTID) REFERENCES ARTGALLERY ON DELETE CASCADE,FOREIGN KEY(EXHIBITIONID) REFERENCES EXHIBITION);
