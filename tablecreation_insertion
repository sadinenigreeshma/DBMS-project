# TABLES AND INSERTION SATEMENTS:
#CUSTOMER TABLE
CREATE TABLE CUSTOMER(CUSTOMERID CHAR(50) PRIMARY KEY, CUSTOMERNAME CHAR(50),PHONENUMBER INT UNIQUE,EMAIL CHAR(50) UNIQUE , ADDRESS CHAR(50));
