# PARTICIPANTS TABLE:
CREATE TABLE PARTICIPATES(ARTISTID CHAR(50),EXHIBITIONID CHAR(50),PRIMARY KEY(ARTISTID,EXHIBITIONID),FOREIGN KEY(ARTISTID) REFERENCES ARTIST ON DELETE CASCADE,FOREIGN KEY(EXHIBITIONID) REFERENCES EXHIBITION);
