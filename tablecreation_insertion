# VIP TABLE:
CREATE TABLE VIP(VISITORID CHAR(50),VISITORNAME CHAR(50),FOREIGN KEY(VISITORID) REFERENCES VISITANT,DATEID DATE,STARTTIME INT,ENDTIME INT,PRIMARY KEY(VISITORID),CHECK (ENDTIME<24),CHECK(STARTTIME<24),CHECK ((ENDTIME-STARTTIME)<4));
INSERT INTO  VIP VALUES('1234A','MEENA','21-OCT-2021',9,12);
INSERT INTO VIP VALUES('1234C','BHANU','30-AUG-2018',12,15);
INSERT INTO VIP VALUES('1234F','MALLI','23-JAN-2019',18,20);
INSERT INTO VIP VALUES('1234G','HARITHA','02-MAY-2020',21,23);
INSERT INTO VIP VALUES('1234E','NANI','11-APR-2017',11,14);

# NORMAL VISITOR TABLE:
CREATE TABLE NORMALVISITOR(VISITORID CHAR(50),VISITORNAME CHAR(50),FOREIGN KEY(VISITORID) REFERENCES VISITANT,DATEID DATE,STARTTIME INT,ENDTIME INT,PRIMARY KEY(VISITORID),CHECK (ENDTIME<24),CHECK(STARTTIME<24));
INSERT INTO NORMALVISITOR VALUES('1234B','RAMU','21-OCT-2021',14,20);
INSERT INTO NORMALVISITOR VALUES('1234D','SRAVYA','04-AUG-2018',16,23);
