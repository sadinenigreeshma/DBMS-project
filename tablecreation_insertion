# PAINTS TABLE:
CREATE TABLE PAINTS(ARTID CHAR(50),ARTISTID CHAR(50) ,PRIMARY KEY(ARTID,ARTISTID),DATEOFPAINTING DATE ,FOREIGN KEY(ARTID) REFERENCES ARTGALLERY ,FOREIGN KEY(ARTISTID) REFERENCES ARTIST ON DELETE CASCADE);
INSERT INTO PAINTS VALUES('ART1','A1','09-OCT-2021');
