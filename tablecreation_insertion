#SIGN UP TABLE:
CREATE TABLE SIGNUP(CUSTOMERID CHAR(50) ,PASSWORD CHAR(50)  UNIQUE,PRIMARY KEY(CUSTOMERID),FOREIGN KEY(CUSTOMERID) REFERENCES CUSTOMER,ARTID CHAR(50),FOREIGN KEY(ARTID) REFERENCES ARTGALLERY);
INSERT INTO SIGNUP VALUES ('101','Hskj$$45','ART2'); 
