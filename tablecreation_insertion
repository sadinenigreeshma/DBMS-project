# ORDERS TABLE:
CREATE TABLE ORDERS(BILLID CHAR(50) PRIMARY KEY,COST INT, TOTALCOST INT  DEFAULT 0,NOOFPAINTINGS INT );
INSERT INTO ORDERS VALUES(12111,12000,15000,8);
