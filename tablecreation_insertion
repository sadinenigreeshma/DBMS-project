# ARTIST ID TABLE:
CREATE TABLE ARTIST(ARTISTID CHAR(50) PRIMARY KEY,ARTISTNAME CHAR(50) UNIQUE,PHONENUMBER INT UNIQUE,ADDRESS CHAR(50));
INSERT INTO ARTIST VALUES ('A1', 'LEONARDO DAVINCI', 6352419872 , 'BANGALORE' );
