# REGISTER TABLE:
CREATE TABLE REGISTER(CUSTOMERID CHAR(50),EXHIBITIONID CHAR(50),PRIMARY KEY(CUSTOMERID,EXHIBITIONID),DATEOFEXHIBITION DATE ,FOREIGN KEY(CUSTOMERID) REFERENCES CUSTOMER,FOREIGN KEY(EXHIBITIONID) REFERENCES EXHIBITION);
