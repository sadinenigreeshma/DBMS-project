# EXHIBITION TABLE:
CREATE TABLE EXHIBITION(EXHIBITIONID CHAR(50) PRIMARY KEY,EXHIBITIONNAME CHAR(50) UNIQUE,DATEID DATE UNIQUE,STARTTIME INT,ENDTIME INT,CHECK ((ENDTIME-STARTTIME)>0),CHECK (STARTTIME<24 AND ENDTIME<24))
INSERT INTO EXHIBITION VALUES('6789','CRAZYARTS','09-OCT-2021',10,23);
